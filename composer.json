{
    "name": "vkal/atributes",
    "description": "Simple PHP 8.4 project with League Router and PHP-DI",
    "type": "project",
    "require": {
        "php": "^8.0",
        "league/route": "^5.1",
        "php-di/php-di": "^6.4",
        "laminas/laminas-diactoros": "^2.8",
        "laminas/laminas-httphandlerrunner": "^1.5",
        "symfony/validator": "^6.0",
        "doctrine/annotations": "^2.0",
        "symfony/console": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.23"
    }
}
